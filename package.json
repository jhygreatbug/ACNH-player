{
  "name": "animal-crossing-new-horizons-player",
  "version": "0.1.1",
  "description": "A player that can simulate the background music of Animal Crossing: New Horizons",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build --mac default --win nsis",
    "electron:build-mac": "vue-cli-service electron:build --mac default",
    "electron:build-win": "vue-cli-service electron:build --win nsis",
    "electron:publish": "vue-cli-service electron:build --mac default --win nsis -p always",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "keywords": [],
  "author": {
    "name": "jhygreatbug",
    "email": "jhygreatbug@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "jsonp": "^0.2.1",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jsonp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "electron": "^11.1.1",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "mini-css-extract-plugin": "^1.3.3",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5"
  }
}
